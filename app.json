{
  "expo": {
    "name": "grocery-buddy",
    "slug": "grocery-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/grocery-buddy-ios.png",
    "scheme": "grocerybuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.diludevx.grocerybuddy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/grocery-buddy-ios.png",
        "backgroundColor": "#4CAF50"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/grocery-buddy-ios.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/grocery-buddy-ios.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#4CAF50"
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a89e5ecd-0ecd-407d-ade0-d2da06844469"
      }
    }
  }
}
